 <!--components/Dialog/dialog.wxml-->
    <view class='wx_dialog_container' hidden='{{!isShow}}'>
        <view class='wx-mask'></view>
        <view class='wx-dialog'>
            <view class='wx-dialog-title'>{{ title }}</view>
            <view class='wx-dialog-content'>{{ content }}</view>
            <view class='wx-dialog-footer'>
              <!--<view class='wx-dialog-btn' bindtap='cancelEvent'>{{ cancelText }}</view>
              <view class='wx-dialog-btn' bindtap='confirmEvent'>{{ confirmText }}</view>-->
              <button class='wx-dialog-btn' bindtap='cancelEvent'>{{ cancelText }}</button>
              <button class='wx-dialog-btn' bindtap='confirmEvent'>{{ confirmText }}</button>
            </view>
        </view>
    </view>